{{test}}

<div class="well">
	{{#buttons}}
	<a class="btn {{class}}" href="{{url}}">{{text}}</a>
	{{/buttons}}
</div>
 

{{# product}}	
    {{{name}}
    {{# photo}}
	<img src={{path_fullsize}} />
	{{path_fullsize}}
    {{/ photo}}
