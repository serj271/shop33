			reviews date {{date}}
		</p>
		<p>
			reviews name {{name}}
		</p>
		<p>
			reviews rating {{rating}}
		</p>
		<p>
			reviews summary  {{summary}}
		</p>
		<p>
			reviews body {{body}}
		</p>
		
		{{/ reviews}}
{{/ product}}
{{{pagination}}}
 

